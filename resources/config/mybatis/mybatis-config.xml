<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>
	
	<typeAliases>
		<typeAlias type="vo.MainVO" alias="main"/>
		<typeAlias type="vo.MemberVO" alias="member"/>
		 <typeAlias type="vo.QnaVO" alias="qna"/> 
		 <typeAlias type="vo.ReviewVO" alias="review"/>
		 <typeAlias type="vo.LikeitVO" alias="likeit"/> 
		 <typeAlias type="vo.MyimgVO" alias="myimg"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="config/mybatis/mapper/main.xml"/>
		<mapper resource="config/mybatis/mapper/member.xml"/>
		<mapper resource="config/mybatis/mapper/qna.xml"/>
		<mapper resource="config/mybatis/mapper/review.xml" />
		<mapper resource="config/mybatis/mapper/likeit.xml" />
	</mappers>
</configuration>